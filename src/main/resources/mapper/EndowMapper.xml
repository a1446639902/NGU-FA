<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yidu.systemManage.mapper.EndowMapper">
<insert id="insertEndow">
    INSERT INTO endow VALUES(xl_endow.nextval,#{param1},#{param2})
</insert>
<delete id="deleteEndow" parameterType="int">
    DELETE FROM endow WHERE roleId=#{roleId}
</delete>


</mapper>