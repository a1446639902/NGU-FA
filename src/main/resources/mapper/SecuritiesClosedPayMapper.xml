<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yidu.businessData.mapper.SecuritiesClosedPayMapper">
   <insert id="insertSecuritiesClosedPay" parameterType="com.yidu.businessData.pojo.SecuritiesClosedPay">
       insert into SecuritiesClosedPay values (#{securitiesClosedPayId},#{fundId},#{accountId},#{securitiesId},#{serviceType},#{amount},#{dateTime},#{flag})
   </insert>
    <delete id="deleteSecuritiesClosedPay" parameterType="com.yidu.businessData.pojo.SecuritiesClosedPay">
        delete from SecuritiesClosedPay where dateTime=#{dateTime} and fundId=#{fundId} and securitiesId=#{securitiesId}
    </delete>
</mapper>